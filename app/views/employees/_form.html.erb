<%=  form_with model: @employee do |form| %>

    <% @employee.errors.full_messages.each do |error| %>
        <div>
            <%= error %>
        </div>
    <% end %>

    <div>
        <%= form.label :email%>
        <%= form.text_field :email %>
    </div>

    <div>
        <%= form.label :name %>
        <%= form.text_field :name %>
    </div>
    <div>
        <%= form.label :position %>
        <%= form.text_field :position %>
    </div>

    <div>
        <%= form.label :employee %>
        <%= form.text_field :employee %>
    </div>

    <div>
        <%= form.label :number_private %>
        <%= form.text_field :number_private %>
    </div>

    <%= form.submit %>
    <a href="<%= employees_path %>"><button>Cancel</button></a>
<% end %>
